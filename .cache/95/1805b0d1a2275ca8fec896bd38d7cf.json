nd-color: var(--bg);\r\n  overflow-x: hidden;\r\n  overflow-y: auto;\r\n  scroll-behavior: smooth;\r\n}\r\nbody::-webkit-scrollbar {\r\n  background-color: var(--bg-color);\r\n  border-radius: 5px;\r\n}\r\nbody::-webkit-scrollbar-thumb {\r\n  background-color: $primary-color;\r\n  border-radius: 5px;\r\n}\r\n","style\\resume.scss":"@import \"./style.scss\";\r\n\r\n#resume {\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: space-between;\r\n}\r\n.resume-content {\r\n  width: 100%;\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n}\r\n.resume-sidebar {\r\n  width: 400px;\r\n  height: 500px;\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: space-around;\r\n  background-color: var(--bg-color);\r\n  padding-right: 1em;\r\n  padding-top: 1em;\r\n  padding-bottom: 1em;\r\n  border-left: 30px solid $primary-color;\r\n}\r\n.section-switch {\r\n  background-color: var(--bg-color);\r\n  color: var(--color);\r\n  width: fit-content;\r\n  border-radius: 50px;\r\n  padding-right: 1em;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n}\r\n.section-switch .switch-icon {\r\n  padding: 1em;\r\n  color: $primary-color;\r\n  font-size: 12px;\r\n  background-color: $primary-color;\r\n  border-top-right-radius: 50px;\r\n  border-bottom-right-radius: 50px;\r\n  margin-right: 1em;\r\n}\r\n.section-switch:hover {\r\n  background-color: $primary-color;\r\n  border-radius: 50px;\r\n  color: var(--white);\r\n}\r\n.resume-sections {\r\n  width: 800px;\r\n  height: 500px;\r\n  overflow: hidden;\r\n  overflow-y: scroll;\r\n  padding: 0em 2em;\r\n  //border-left: 5px solid $primary-color;\r\n}\r\n.resume-sections::-webkit-scrollbar {\r\n  background-color: var(--bg-color);\r\n  box-shadow: 0px 3px 6px rgb(0, 0, 0, 0.1);\r\n  width: 15px;\r\n  border-radius: 12px;\r\n}\r\n.resume-sections::-webkit-scrollbar-thumb {\r\n  background-color: $primary-color;\r\n  border-radius: 12px;\r\n}\r\n.resume-section {\r\n  width: 100%;\r\n  height: 500px;\r\n}\r\n.resume-card {\r\n  padding: 1em 0em;\r\n  display: flex;\r\n  position: relative;\r\n}\r\n.dot-card {\r\n  width: 20px;\r\n  height: 20px;\r\n  background-color: $primary-color;\r\n  position: absolute;\r\n  left: -30px;\r\n  top: 15px;\r\n  border-radius: 50px;\r\n}\r\n.card-col {\r\n  width: 85%;\r\n}\r\n.resume-card-heading {\r\n  font-size: 20px;\r\n  font-weight: 600;\r\n  color: $secondary-color;\r\n  margin-bottom: 0.5em;\r\n}\r\n.resume-card-info {\r\n  color: var(--color);\r\n  font-size: 18px;\r\n}\r\n.resume-card-explain {\r\n  font-size: 17px;\r\n  color: var(--color);\r\n}\r\n.resume-section-line {\r\n  width: 80%;\r\n  height: 3px;\r\n  border-radius: 12px;\r\n  margin: 0px auto;\r\n  background-color: var(--bg-color);\r\n}\r\n.date {\r\n  padding: 0.5em 0.9em;\r\n  background-color: $secondary-color;\r\n  font-size: 16px;\r\n  border-radius: 50px;\r\n  width: fit-content;\r\n  height: fit-content;\r\n  color: var(--white);\r\n}\r\n@media (max-width: $xl-width) {\r\n  .resume-sidebar {\r\n    display: flex;\r\n  }\r\n}\r\n@media (max-width: $lg-width) {\r\n  .resume-sidebar {\r\n    display: flex;\r\n  }\r\n}\r\n@media (max-width: $md-width) {\r\n  .resume-sidebar {\r\n    display: none;\r\n  }\r\n  .resume-content {\r\n    justify-content: center;\r\n  }\r\n  .resume-sections {\r\n    width: 80%;\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"7143a5f65e634edecb618491f6a44875","cacheData":{"env":{}}}