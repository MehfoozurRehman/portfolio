y-content: space-between;\r\n    padding: 2em 2.5em;\r\n  }\r\n}\r\n@media (max-width: 650px) {\r\n  .form {\r\n    right: 80px;\r\n  }\r\n}\r\n*/\r\n","style\\jumbotron.scss":"@import \"./style.scss\";\r\n\r\n#jumbotron {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  .about_me {\r\n    font-size: 30px;\r\n    text-transform: capitalize;\r\n    letter-spacing: 5px;\r\n    color: $primary-color;\r\n  }\r\n  .slogan {\r\n    font-size: 40px;\r\n    font-weight: 600;\r\n    line-height: 45px;\r\n    color: $secondary-color;\r\n    letter-spacing: 3px;\r\n    padding: 0.3em 0em;\r\n  }\r\n  .info {\r\n    letter-spacing: 2px;\r\n    font-size: 20px;\r\n    display: block;\r\n    color: var(--color);\r\n  }\r\n  .cta {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    margin-top: 50px;\r\n    width: 330px;\r\n  }\r\n  .cta a {\r\n    padding: 0.7em;\r\n    border-radius: 20px;\r\n    width: 45%;\r\n    font-size: 18px;\r\n    text-align: center;\r\n  }\r\n  .primary-cta {\r\n    background-color: $primary-color;\r\n    color: white;\r\n  }\r\n  .primary-cta:hover {\r\n    background-color: darken($primary-color, 20%);\r\n  }\r\n  .secondary-cta {\r\n    background-color: $secondary-color;\r\n    color: white;\r\n  }\r\n  .secondary-cta:hover {\r\n    background-color: darken($secondary-color, 20%);\r\n  }\r\n  .jumbotron-pic {\r\n    border-radius: 1000px;\r\n    border: 20px solid var(--bg-color);\r\n    box-shadow: 0px 3px 6px rgb(0, 0, 0, 0.1);\r\n    width: 380px;\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"ae6d0f5c28ee5de687c2270fc77e5b46","cacheData":{"env":{}}}