\n  text-decoration: none;\r\n}\r\nbody {\r\n  margin: 0;\r\n  padding: 0;\r\n  width: 100vw;\r\n  height: 100%;\r\n  font-family: sans-serif;\r\n  background-color: var(--bg);\r\n  overflow-x: hidden;\r\n  overflow-y: auto;\r\n  scroll-behavior: smooth;\r\n}\r\nbody::-webkit-scrollbar {\r\n  background-color: var(--bg-color);\r\n  border-radius: 5px;\r\n}\r\nbody::-webkit-scrollbar-thumb {\r\n  background-color: $primary-color;\r\n  border-radius: 5px;\r\n}\r\n","style\\header.scss":"@import \"./style.scss\";\r\n\r\n.header {\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: center;\r\n  align-items: center;\r\n  width: 100vw;\r\n  position: fixed;\r\n  top: 0;\r\n  background-color: var(--bg-color);\r\n  box-shadow: 0px 3px 10px rgba(0, 0, 0, 0.2);\r\n  z-index: 1;\r\n  padding-right: 15px;\r\n}\r\n.nav {\r\n  width: 100%;\r\n  max-width: $xl-width;\r\n  height: 70px;\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n}\r\ninput[type=\"checkbox\"] {\r\n  height: 0;\r\n  width: 0;\r\n  visibility: hidden;\r\n}\r\n\r\n.label {\r\n  cursor: pointer;\r\n  text-indent: -9999px;\r\n  width: 52px;\r\n  height: 23px;\r\n  background: $primary-color;\r\n  float: right;\r\n  border-radius: 100px;\r\n  position: relative;\r\n}\r\n\r\n.label:after {\r\n  content: \"\";\r\n  position: absolute;\r\n  top: 3px;\r\n  left: 3px;\r\n  width: 18px;\r\n  height: 17px;\r\n  background: var(--bg-color);\r\n  border-radius: 90px;\r\n  transition: 0.3s;\r\n}\r\n\r\ninput:checked + .label {\r\n  background: $primary-color;\r\n}\r\n\r\ninput:checked + .label:after {\r\n  left: calc(100% - 5px);\r\n  transform: translateX(-100%);\r\n}\r\n\r\n.label:active:after {\r\n  width: 45px;\r\n}\r\n\r\nhtml.transition,\r\nhtml.transition *,\r\nhtml.transition *:before,\r\nhtml.transition *:after {\r\n  transition: all 750ms !important;\r\n  transition-delay: 0 !important;\r\n}\r\n.brand-name {\r\n  font-size: $heading-font-size;\r\n  font-weight: bold;\r\n  color: $primary-color;\r\n  letter-spacing: 2px;\r\n  text-decoration: none;\r\n}\r\n.menu-btn {\r\n  margin-right: 20px;\r\n  position: relative;\r\n  display: none;\r\n  justify-content: center;\r\n  align-items: center;\r\n  width: 30px;\r\n  height: 25px;\r\n  outline: none;\r\n  cursor: pointer;\r\n  transition: all 0.5s ease-in-out;\r\n  /* border: 3px solid #fff; */\r\n}\r\n.menu-btn__burger {\r\n  width: 25px;\r\n  height: 3px;\r\n  background: $primary-color;\r\n  border-radius: 5px;\r\n  box-shadow: 0 2px 5px rgba(0, 0, 0, 0.288);\r\n  transition: all 0.5s ease-in-out;\r\n}\r\n.menu-btn__burger::before,\r\n.menu-btn__burger::after {\r\n  content: \"\";\r\n  position: absolute;\r\n  width: 30px;\r\n  height: 3px;\r\n  background: $primary-color;\r\n  border-radius: 5px;\r\n  box-shadow: 0 2px 5px rgba(0, 0, 0, 0.288);\r\n  transition: all 0.5s ease-in-out;\r\n}\r\n.menu-btn__burger::before {\r\n  transform: translateY(-10px);\r\n}\r\n.menu-btn__burger::after {\r\n  transform: translateY(10px);\r\n}\r\n/* ANIMATION */\r\n.menu-btn.open .menu-btn__burger {\r\n  transform: translateX(-50px);\r\n  background: transparent;\r\n  box-shadow: none;\r\n}\r\n.menu-btn.open .menu-btn__burger::before {\r\n  transform: rotate(45deg) translate(35px, -35px);\r\n}\r\n.menu-btn.open .menu-btn__burger::after {\r\n  transform: rotate(-45deg) translate(35px, 35px);\r\n}\r\n.desktop-nav {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  width: fit-content;\r\n}\r\n.nav-link {\r\n  font-size: 15px;\r\n  text-decoration: none;\r\n  background-color: var(--bg-color);\r\n  color: var(--color);\r\n  padding: 0px 1em;\r\n}\r\n.nav-link:hover {\r\n  color: $primary-color;\r\n}\r\n.mobile-nav {\r\n  display: none;\r\n  flex-direction: column;\r\n  width: 100vw;\r\n  padding: 1em auto;\r\n  box-shadow: 0px 3px 10px rgba(0, 0, 0, 0.2);\r\n}\r\n.mobile-nav-link {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  width: 100%;\r\n  height: 6vh;\r\n  font-size: 15px;\r\n  text-decoration: none;\r\n  background-color: var(--bg-color);\r\n  color: var(--color);\r\n}\r\n.mobile-nav-link:hover {\r\n  background-color: $primary-color;\r\n  color: var(--bg-color);\r\n}\r\n@media (max-width: $xl-width) {\r\n  .nav {\r\n    max-width: $lg-width;\r\n  }\r\n}\r\n@media (max-width: $lg-width) {\r\n  .nav {\r\n    max-width: $md-width;\r\n  }\r\n  .brand-name {\r\n    padding: 0px 0.5em;\r\n  }\r\n}\r\n@media (max-width: $md-width) {\r\n  .nav {\r\n    max-width: $md-width;\r\n  }\r\n  .desktop-nav {\r\n    display: none;\r\n  }\r\n  .menu-btn {\r\n    display: flex;\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"edcae1699d54bd46a32d55d143cc7abb","cacheData":{"env":{}}}